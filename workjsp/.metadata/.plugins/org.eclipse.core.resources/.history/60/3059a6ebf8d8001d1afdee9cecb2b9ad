package utils;

import com.oreilly.servlet.MultipartRequest;

public class FileUtil 
{
	//파일 업로드 처리
	public static MultipartRequest uploadFile(HTttpServletRequset req, String saveDirectory, int maxPostSize)
	{
		return new MultipartRequest(req, saveDirectory, maxPostSize, "UTF-8");
	}
	//파일 다운로드 
	//파일 삭제
	
}
